{
  "extends": ["airbnb-base"],
  "env": { "browser": true },
  "rules": {
    "no-console": ["error", { allow: ["warn", "error"] }],
    "func-names": ["error", "as-needed"],
    "max-len": ["error", { "code": 160 }]
  },
  "globals": {
   "browser": true,
   "chrome": true,
   "window": true,
   "document": true
  }
}
